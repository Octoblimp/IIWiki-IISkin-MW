{
	"name": "IISkin",
	"version": "1.0.0",
	"author": [
		"IIWiki Contributors"
	],
	"url": "https://iiwiki.com",
	"descriptionmsg": "iiskin-desc",
	"namemsg": "skinname-iiskin",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"ValidSkinNames": {
		"iiskin": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "iiskin",
					"responsive": true,
					"styles": [ "skins.iiskin" ],
					"scripts": [ "skins.iiskin.js" ],
					"templateDirectory": "skins/IISkin/templates"
				}
			]
		}
	},
	"MessagesDirs": {
		"IISkin": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.iiskin": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content": true,
				"content-media": true,
				"interface": true,
				"interface-category": true,
				"interface-message-box": true,
				"logo": false,
				"toc": true
			},
			"styles": [
				"resources/skin.css"
			]
		},
		"skins.iiskin.js": {
			"scripts": [
				"resources/skin.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "IISkin"
	},
	"manifest_version": 2
}
