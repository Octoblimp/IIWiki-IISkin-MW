{
	"name": "IISkin",
	"version": "4.1.1",
	"author": [
		"Makko Oko"
	],
	"url": "https://iiwiki.com",
	"descriptionmsg": "iiskin-desc",
	"namemsg": "skinname-iiskin",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"ValidSkinNames": {
		"iiskin": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "iiskin",
					"responsive": true,
					"supportsMwHeading": true,
					"styles": ["skins.iiskin"],
					"scripts": ["skins.iiskin.js"],
					"templateDirectory": "templates",
					"toc": false
				}
			]
		}
	},
	"MessagesDirs": {
		"IISkin": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.iiskin": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content": true,
				"content-links": true,
				"content-media": true,
				"content-tables": true,
				"content-parser-output": true,
				"interface": true,
				"interface-category": true,
				"interface-message-box": true,
				"interface-edit-section-links": true,
				"logo": false,
				"toc": false
			},
			"styles": [
				"resources/skin.css"
			]
		},
		"skins.iiskin.js": {
			"scripts": [
				"resources/skin.js"
			],
			"dependencies": [
				"mediawiki.cookie",
				"mediawiki.searchSuggest",
				"mediawiki.storage",
				"mediawiki.util"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "IISkin"
	},
	"manifest_version": 2
}
